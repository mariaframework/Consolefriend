{
    "name": "mariaframework/friendconsole",
    "description": "Componente que vai te auxiliar no desenvolvimento de suas aplicações baseadas no maria framework e támbem zend Expressive",
    "license": "MIT",
    "authors": [
        {
            "name": "José Tiago",
            "email": "josetiagobsouza@gmail.com"
        }
    ],
    "require": {
        "symfony/console":"3.0.*",
        "league/flysystem": "^1.0",
        "symfony/finder": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "MariaFW\\FriendConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8888 -t public/ public/index.php",
        "test": "phpunit",
        "new friend":""
    }
}
